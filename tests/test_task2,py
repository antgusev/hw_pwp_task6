from unittest import TestCase
# import requests
from my_ya_disk import response


class Test_response(TestCase):
    def test_response(self):
        res = response
        expected = 400
        self.assertLess(res, expected)
        if res >= 200 and res < 400:
            print(f'Код ответа соответствует ', {res})
        elif res == 400:
            print(f'Код ответа', {res}, ': Некорректные данные')
        elif res == 401:
            print(f'Код ответа', {res}, ': Не авторизован')
        else:
            print(f'Код ответа', {res}, ': Ошибка, см. на ресурсе: https://yandex.ru/dev/disk/poligon/')
